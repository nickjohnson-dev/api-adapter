{
  "name": "api-adapter",
  "version": "1.1.0",
  "description": "Decouple the shape of your data on the client and server",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --ignore test.js",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "prebuild": "npm run clean -s",
    "prerelease": "npm run lint -s && npm run testonce -s && npm run build -s",
    "release": "npm publish",
    "test": "ava --watch",
    "testonce": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickjohnson-dev/api-adapter.git"
  },
  "keywords": [],
  "author": {
    "name": "Nick Johnson",
    "email": "nickjohnson.dev@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nickjohnson-dev/api-adapter/issues"
  },
  "homepage": "https://github.com/nickjohnson-dev/api-adapter#readme",
  "devDependencies": {
    "ava": "0.18.1",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.0",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-es2017": "6.22.0",
    "babel-preset-stage-3": "6.22.0",
    "eslint": "3.15.0",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-lodash-fp": "2.1.3",
    "sinon": "2.0.0-pre.5"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "lodash": "4.17.15"
  }
}
